  325  
  326:   constructor(private router: Router) {
  327:     router.events.pipe(takeUntil(this._unsubscribe$)).subscribe(event => {
  328:       if (event instanceof NavigationEnd) {
  329:         this.url = event.url;
  330        }

  333  
  334:   ngOnDestroy(): void {
  335:     this._unsubscribe$.next();
  336:     this._unsubscribe$.complete();
  337    }
  338  
  339:   join(...routes: Route[]): string {
  340:     return `${this._root}/${routes.map(r => {
  341:       return r.route ? r.route : r.name.toLowerCase().replace(/ +(& +)?/g, '-');
  342:     }).join('/')}`;
  343    }

